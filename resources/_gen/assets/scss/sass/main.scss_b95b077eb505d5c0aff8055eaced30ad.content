@use 'colors';nav ul{list-style:none;display:flex;align-items:center;font-family:Raleway;padding-left:0}nav ul li{font-weight:700}nav ul li:first-child{flex-grow:1}nav ul li:not(:first-child) a{margin-left:1em;padding:.5em;transition:background-color .2s ease-in;border-radius:.1em;cursor:pointer}nav ul li:not(:first-child) a:hover{background:var(--hover-bg)}nav ul li a{color:inherit}body{--nord-0: #2e3440;--nord-1: #3b4252;--nord-2: #434c5e;--nord-3: #4c566a;--nord-4: #d8dee9;--nord-5: #e5e9f0;--nord-6: #eceff4;--nord-7: #8fbcbb;--nord-8: #88c0d0;--nord-9: #81a1c1;--nord-10: #5e81ac;--nord-11: #bf616a;--nord-12: #d08770;--nord-13: #ebcb8b;--nord-14: #a3be8c;--nord-15: #b48ead;--bg: var(--nord-6);--hover-bg: var(--nord-4);--fg: var(--nord-0)}body.dark{--bg: var(--nord-0);--hover-bg: var(--nord-3);--fg: var(--nord-4)}body{background:var(--bg);color:var(--fg);font-family:IBM Plex Sans Condensed;margin:2em auto;width:80%;font-size:1.2em}a{text-decoration:none}